{
  "name": "corgi",
  "version": "0.1.0",
  "private": true,
  "main": "tailwind.config.js",
  "dependencies": {
    "@inlet/react-pixi": "^6.6.5",
    "@material-ui/core": "^4.11.3-deprecations.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.13",
    "pixi-spine": "^3.0.11",
    "pixi.js": "^6.1.2",
    "postcss-import": "^14.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "CI=false && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "postcss src/assets/css/main.scss -o src/assets/css/styles.css",
    "watch:dev": "nodemon -x npm run dev -w tailwind.config.js -w src/assets/css/main.scss -w src/assets/css/utilities.scss",
    "prod": "cross-env NODE_ENV=production postcss src/assets/css/main.scss -o src/assets/css/styles.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "autoprefixer": "^10.3.7",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "react": "^17.0.2",
    "react-date-countdown-timer": "^1.0.9",
    "react-router-dom": "^5.3.0",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  }
}
